Panels:
  - Class: rviz_common/Displays
    Name: Displays
    Property Tree Widget:
      Expanded:
        - /MotionPlanning
  - Class: rviz_common/Help
    Name: Help
  - Class: rviz_common/Views
    Name: Views

Visualization Manager:
  Displays:
    - Class: rviz_default_plugins/Grid
      Name: Grid
      Value: true

    - Class: moveit_rviz_plugin/MotionPlanning
      Name: MotionPlanning
      Planned Path:
        Loop Animation: true
        State Display Time: 0.05 s
        Query Goal State: false
        Trajectory Topic: display_planned_path
      Planning Scene Topic: monitored_planning_scene
      Robot Description: robot_description
      Scene Geometry:
        Scene Alpha: 1
        Show Scene Geometry: true
        Voxel Rendering: Occupied Voxels
        Voxel Coloring: Z-Axis
      Scene Robot:
        Robot Alpha: 0.5
      Value: true

    # -----------------------------
    # Octomap displays (OFF by default)
    # -----------------------------
    - Class: rviz_default_plugins/MarkerArray
      Name: Octomap - Occupied Cells (octomap_server)
      Enabled: false
      Value: false
      Marker Topic: /occupied_cells_vis_array
      Queue Size: 10

    - Class: rviz_default_plugins/Map
      Name: Octomap - Projected Map (octomap_server)
      Enabled: false
      Value: false
      Topic: /projected_map
      Alpha: 0.7

    # -----------------------------
    # Perception displays (OFF by default)
    # -----------------------------
    - Class: rviz_default_plugins/PointCloud2
      Name: Perception - D405 PointCloud (/d405/depth/color/points)
      Enabled: false
      Value: false
      Topic: /d405/depth/color/points
      Queue Size: 10
      Selectable: true
      Style: Flat Squares
      Size (Pixels): 3
      Size (m): 0.01
      Alpha: 0.7
      Color Transformer: RGB8
      Position Transformer: XYZ
      Use Fixed Frame: true

    - Class: rviz_default_plugins/PointCloud2
      Name: Perception - D405 PointCloud (raw /d405/points)
      Enabled: false
      Value: false
      Topic: /d405/points
      Queue Size: 10
      Selectable: true
      Style: Flat Squares
      Size (Pixels): 3
      Size (m): 0.01
      Alpha: 0.7
      Color Transformer: RGB8
      Position Transformer: XYZ
      Use Fixed Frame: true

    - Class: rviz_default_plugins/Image
      Name: Perception - D405 Depth Image
      Enabled: false
      Value: false
      Image Topic: /d405/depth_image
      Transport Hint: raw
      Queue Size: 2
      Normalize Range: true
      Min Value: 0
      Max Value: 1
      Median window: 5
      Unreliable: false

    - Class: rviz_default_plugins/Image
      Name: Perception - D405 RGB Image
      Enabled: false
      Value: false
      Image Topic: /d405/rgb/image_raw
      Transport Hint: raw
      Queue Size: 2
      Normalize Range: true
      Min Value: 0
      Max Value: 1
      Median window: 5
      Unreliable: false

    - Class: rviz_default_plugins/TF
      Name: TF
      Enabled: false
      Value: false
      Frame Timeout: 15.0
      Show Arrows: true
      Show Axes: true
      Show Names: true

  Global Options:
    Fixed Frame: world

  Tools:
    - Class: rviz_default_plugins/Interact
    - Class: rviz_default_plugins/MoveCamera
    - Class: rviz_default_plugins/Select

  Views:
    Current:
      Class: rviz_default_plugins/Orbit
      Distance: 2.0
      Focal Point:
        X: -0.1
        Y: 0.25
        Z: 0.30
      Name: Current View
      Pitch: 0.5
      Target Frame: world
      Yaw: -0.623

Window Geometry:
  Height: 975
  Width: 1200
  QMainWindow State: 
